function OpenAuthWin(URL) {
   return (open(URL,"ClientCTRL","top=400,left=500,width=450,height=240,scrollbars=no,menubar=no,toolbar=no,statusbar=no"));
}
function Authenticate() {
  if (""=="yes") {
     alert("This is a Captive Portal Web Login preview.");
     return;
  }
  X=document.data;
  if (X.U.value=="") {
    alert("The Username cannot be empty.");
    X.U.focus();
    return;
  }
  if (X.P.value=="") {
    alert("The Password cannot be empty.");
    X.P.focus();
    return;
  }
  newwin=OpenAuthWin("/script/connected.cgi?"+X.U.value+""); <!-- chu y dong code nay -->
  X.submit();	 <!-- chu y dong code nay -->
  newwin.focus();	 <!-- chu y dong code nay -->
  
}
function loaded() {
  ImgLoaded();
  X=document.data;
  if (""=="yes") {
     X.U.disabled=true;    
     X.P.disabled=true;    
  } else {
    X.U.focus();
    X.ZSCPRedirect.value=unescape(X.ZSCPRedirect.value);
    X.RND.value=Math.random();
  }
  
}
function CheckEnterKey(FieldObj,Event) {
  var keycode;
  if (window.event) {
    keycode = window.event.keyCode;
  } else {
    if (Event) {
      keycode = Event.which;
    } else {
      return true;
    }
  }
  if (keycode == 13) {
   if (FieldObj==document.data.P) {
     Authenticate();
     return false;
   } else if (FieldObj==document.data.U) {
     document.data.P.focus();
     return false;
   } 
   return true;
  } else {
   return true;
  }
}
function ImgLoaded() {
  document["Immagine"].src="/cp_image";
  largo=parseInt("160");
  if (largo=="NaN") {
    largo=240;
  }
  alto=parseInt("100");
  if (alto=="NaN") {
    alto=160;
  } 
  document["Immagine"].width=largo;
  document["Immagine"].height=alto;
}
function ShowInfo() {
   var newwin=window.open("zscp?Section=CPAuth&Action=ShowInfo&Type=General","ShowInfo","top=200,left=300,width=550,height=400,scrollbars=yes,menubar=no,toolbar=no,statusbar=no");
   newwin.focus();
}
function x509auth() {
  if (""=="yes") {
     alert("This is a Captive Portal Web Login preview.");
     return;
  }
  X=document.data;
  DURL="/SSLAuthClient/cgi-bin/zscp?Section=CPAuth&Action=Authenticate&ZSCPRedirect="+escape(X.ZSCPRedirect.value)+"&Powered="+X.Powered.value+"&RND="+X.RND.value;
  location=DURL
  newwin=OpenAuthWin("");
  newwin.focus();
}
